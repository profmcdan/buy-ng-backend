{
  "name": "sick-fits-backend",
  "version": "1.0.0",
  "description": "the backend of sick fits",
  "scripts": {
    "start": "nodemon -e js,graphql -x node src/index.js",
    "debug": "nodemon -e js,graphql -x node --inspect src/index.js",
    "test": "jest",
    "playground": "graphql playground --dotenv",
    "deploy": "prisma deploy --env-file .env",
    "dev": "npm-run-all --parallel debug playground"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "6.0.0",
    "graphql": "^14.2.1",
    "graphql-cli": "^2.16.7",
    "graphql-import": "^0.7.1",
    "graphql-yoga": "^1.17.4",
    "jsonwebtoken": "8.3.0",
    "nodemailer": "^4.6.8",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5",
    "prisma-binding": "^2.3.10",
    "prisma-client-lib": "^1.30.0",
    "stripe": "^6.12.1"
  },
  "license": "MIT",
  "repository": {
    "url": "profmcdan.com"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0"
  }
}
