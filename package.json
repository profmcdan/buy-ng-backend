{
  "name": "sick-fits-backend",
  "version": "1.0.0",
  "description": "the backend of sick fits",
  "scripts": {
    "start": "nodemon -e js,graphql -x node index.js",
    "debug": "nodemon -e js,graphql -x node --inspect index.js",
    "test": "jest",
    "playground": "graphql playground --dotenv",
    "deploy": "prisma deploy --env-file .env",
    "dev": "npm-run-all --parallel debug playground"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.4",
    "dotenv": "^7.0.0",
    "graphql": "^14.2.1",
    "graphql-cli": "^3.0.11",
    "graphql-import": "^0.7.1",
    "graphql-yoga": "^1.17.4",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.1.0",
    "npm-run-all": "^4.1.5",
    "prisma-client-lib": "^1.30.0",
    "stripe": "^6.28.0"
  },
  "license": "MIT",
  "repository": {
    "url": "profmcdan.com"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "babel-preset-env": "^1.7.0"
  }
}
